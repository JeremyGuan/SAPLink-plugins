  METHOD conv_vbs_tables_st .
    DATA:
      lv_line               TYPE string ,
      lt_table              TYPE ty_t_string_tab ,
      ls_vbs_tables_tab     TYPE ty_s_vbs_tables_tab .
    CLEAR rt_vbs_tables_tab[] .

    SPLIT iv_vbs_tables AT &apos;|&apos; INTO TABLE lt_table .
