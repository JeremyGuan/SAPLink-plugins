                                  pv_display     TYPE any
                       CHANGING   cv_returncode  TYPE any
                                  cv_value       TYPE any .

  DATA: lv_objname    TYPE rsrd1-objname .

  CALL FUNCTION &apos;RS_DD_F4_OBJECT&apos;
    EXPORTING
      objname            = lv_objname
      objtype            = &apos;Q&apos;
      suppress_selection = space
    IMPORTING
      selobjname         = lv_objname.

  cv_value = lv_objname .

ENDFORM .                    &quot;popup_context_f4</include_source>
