                                             table_type = i_oi_table_collection=&gt;table_type_output
                                   IMPORTING error      = lr_error
                                   CHANGING  data_table = &lt;lt_table_data_copy&gt; ) .
        msg_io_catch( lr_error ) .
        CHECK v_retcode NE c_retcode-error .
