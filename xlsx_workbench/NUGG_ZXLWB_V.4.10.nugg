        wsdr              TYPE REF TO if_ixml_element ,
        any               TYPE REF TO if_ixml_element ,
        row               TYPE REF TO if_ixml_element ,
        cols              TYPE REF TO if_ixml_element ,
        col               TYPE REF TO if_ixml_element ,
