      ls_colprop            TYPE ty_s_colprop ,
      ls_rowprop            TYPE ty_s_rowprop .
    FIELD-SYMBOLS:
      &lt;cells&gt;               TYPE ty_s_cells ,
      &lt;sharedstrings&gt;       TYPE ty_s_sharedstrings ,
      &lt;colprop&gt;             TYPE ty_s_colprop ,
