    ls_excl_f4-func = &apos;&amp;ILT&apos; . APPEND ls_excl_f4 TO lt_excl_f4 .
    ls_excl_f4-func = &apos;&amp;ELP&apos; . APPEND ls_excl_f4 TO lt_excl_f4 .

    CLEAR ls_fct_f4 .
