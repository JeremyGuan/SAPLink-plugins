                                                          iv_bitmap    = abap_on ) .
        WHEN r_formtemplate-&gt;c_comp_type-chart .
          ls_form_prop-tb_drwstat_name = popup_tbdrawing( iv_sheetname = lv_tb_sheetname
                                                          iv_chart     = abap_on ) .
