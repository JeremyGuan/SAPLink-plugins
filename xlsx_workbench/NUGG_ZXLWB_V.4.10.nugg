         WITH v_text-t079                   &quot; text: Appearance in runtime:
              v_text-t020  INTO v_dummy .   &quot; text: Context binding is incorrect
      msg_syst_catch2( CHANGING ct_retmess = cs_data-exceptions-t_retmess ) .
