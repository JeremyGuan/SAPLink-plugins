      lr_element-lpstr =
      s_model-ixml_doc_app-&gt;create_simple_element(
                        namespace = c_tag-vt    name = c_tag-lpstr
                        parent    = lr_element-variant ) .

      lv_string_tmp = v_text-t007 . &quot; text: Sheets
      lr_element-lpstr-&gt;set_value( lv_string_tmp ) .

      lr_element-variant =
      s_model-ixml_doc_app-&gt;create_simple_element(
